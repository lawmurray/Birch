/*
 * Iterator over an arbitrary empty container.
 */
final class EmptyIterator<Type> < Iterator<Type> {
  /**
   * Is there a next element?
   */
  override function hasNext() -> Boolean {
    return false;
  }

  /**
   * Get the next element.
   */
  override function next() -> Type {
    assert(false);
    return Type();
  }
}
